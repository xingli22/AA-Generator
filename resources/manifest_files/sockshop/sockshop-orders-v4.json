{
  "service": "orders",
  "version": "v4",
  "requests": [
    {
      "type": "http",
      "url": "http://payment/paymentAuth",
      "name": "payment",
      "path": "/paymentAuth",
      "method": "POST"
    },
    {
      "type": "http",
      "url": "http://shipping/shipping",
      "name": "shipping",
      "path": "/shipping",
      "method": "POST"
    },
    {
      "type": "tcp",
      "url": "mongodb://orders-db:27017/data",
      "name": "orders-db",
      "port": "27017"
    },
    {
      "type": "http",
      "url": "http://user/customers/*",
      "name": "user",
      "path": "*",
      "method": "GET"
    },
    {
      "type": "http",
      "url": "http://carts/carts/*/items",
      "name": "carts",
      "path": "*",
      "method": "GET"
    },
    {
      "type": "http",
      "url": "http://carts/carts/*/items",
      "name": "catalog",
      "path": "*",
      "method": "GET"
    }
  ]
}